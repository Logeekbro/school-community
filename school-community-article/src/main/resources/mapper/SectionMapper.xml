<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.db.dbcommunity.article.mapper.SectionMapper">

    <resultMap id="BaseResultMap" type="com.db.dbcommunity.article.model.entity.Section">
            <id property="sectionId" column="section_id" jdbcType="INTEGER"/>
            <result property="sectionName" column="section_name" jdbcType="CHAR"/>
            <result property="createBy" column="create_by" jdbcType="BIGINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="deleted" column="deleted" jdbcType="BIT"/>
    </resultMap>

    <sql id="Base_Column_List">
        section_id,section_name,create_by,
        create_time,update_time,deleted
    </sql>
</mapper>
